{
    "name": "juko-labs/future-shop",
    "description": "Future shop is a WordPress plugin built for running an online store using Stripe.",
    "type": "wordpress-plugin",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Justin Kopepasah",
            "email": "justin@kopepasah.com"
        }
    ],
    "autoload": {
        "files": [
            "lib/system/compatability.php"
        ],
        "psr-4": {
            "FutureShop\\": "src"
        }
    },
    "require": {
        "stripe/stripe-php": "^7.37"
    },
    "require-dev": {
        "juko-labs/php-coding-standards": "^0.1.2"
    },
    "scripts": {
        "phpcs": "./vendor/bin/phpcs ."
    }
}
