{
    "extends": [
        "plugin:@wordpress/eslint-plugin/recommended-with-formatting"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "wp": true
    },
    "rules": {
        "no-console": "warn",
        "quote-props": ["error", "consistent"],
        "key-spacing": [
            "error",
            {
                "beforeColon": true,
                "align": "colon"
            }
        ]
    }
}
